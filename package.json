{
  "name": "base-t3-mantine-web3",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "start": "next start",
    "lint": "next lint",
    "prisma-push": "npx prisma db push",
    "prisma-studio": "npx prisma studio",
    "prisma-generate": "prisma generate",
    "typechain": "typechain --target ethers-v5 --input-dir src/abis --out-dir src/typechain-non-owned './src/abis/*.json'",
    "format-contract": "npx prettier --write blockchain/contracts/*.sol",
    "hardhat-compile": "cd blockchain && npx hardhat compile",
    "hardhat-clean": "cd blockchain && npx hardhat clean && rm -r temp",
    "deploy-token-bnbt": "yarn hardhat-clean && cd blockchain/scripts && npx --network=bnbt hardhat run token.ts --network bnbt",
    "deploy-mint-bnbt": "yarn hardhat-clean && cd blockchain/scripts && npx --network=bnbt hardhat run mint.ts --network bnbt",
    "deploy-mintDuplicate-bnbt": "yarn hardhat-clean && cd blockchain/scripts && npx --network=bnbt hardhat run mintDuplicate.ts --network bnbt",
    "deploy-staking-bnbt": "yarn hardhat-clean && cd blockchain/scripts && npx --network=bnbt hardhat run staking.ts --network bnbt"
  },
